 * @version    SVN: $Id: actions.class.php 23810 2009-11-12 11:07:44Z Kris.Wallsmith $
 */
class searchActions extends sfActions
{
 /**
  * Executes index action
  *
  * @param sfRequest $request A request object
  */
  public function executeIndex(sfWebRequest $request)
  {
    //$this->forward('default', 'module');

    $city = $request->getParameter('Keywords');
    $minprice = $request->getParameter('MinPrize');
    $maxprice = $request->getParameter('MaxPrize');
    $bedrooms = $request->getParameter('Bedrooms');
   
    //database insertion call.
     $dataInsert= new SEARCH();
	list($this->count,$this->dataarr) = $dataInsert->searchTable($city, $minprice, $maxprice, $bedrooms);
	$this->dataarr = json_decode(json_encode($this->dataarr),TRUE);

  }
public function executeShow(sfWebRequest $request)
  {}

}
